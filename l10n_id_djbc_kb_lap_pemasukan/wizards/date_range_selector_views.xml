<openerp>
    <data>
        <record id="kb_lap_pemasukan_wizard_view_form" model="ir.ui.view">
            <field name="name">l10n_id.kb_lap_pemasukan_wizard form</field>
            <field name="model">l10n_id.kb_lap_pemasukan_wizard</field>
            <field name="inherit_id" ref="l10n_id_djbc_app.date_range_selector_view_form"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//button[@special='cancel']" position="before">
                        <button name="action_lap_pemasukan"
                            type="object"
                            string="Print"
                            class="oe_hightlight"/>
                    </xpath>
                </data>
            </field>
        </record>
        
        <record id="kb_lap_pemasukan_wizard_action" model="ir.actions.act_window">
            <field name="name">Pemasukan Barang</field>
            <field name="res_model">l10n_id.kb_lap_pemasukan_wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="kb_lap_pemasukan_wizard_view_form"/>
            <field name="target">new</field>
        </record>

        <menuitem
            id="kb_lap_pemasukan_barang_menu"
            name="Pemasukan Barang"
            parent="l10n_id_djbc_kb_lap_common.djbc_reporting_kb_menu"
            action="kb_lap_pemasukan_wizard_action"
            sequence="5"
        />
    </data>
</openerp>
