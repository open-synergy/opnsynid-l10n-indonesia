<openerp>
<data>

<record id="cetak_lap_posisi_barang_common_view_form" model="ir.ui.view">
    <field name="name">l10n_id.cetak_lap_posisi_barang_common form</field>
    <field name="model">l10n_id.cetak_lap_posisi_barang_common</field>
    <field name="arch" type="xml">
        <form>
            <group name="group_1" colspan="4" col="2">
                <field name="company_id" />
                <field name="date_start" />
                <field name="date_end" />
